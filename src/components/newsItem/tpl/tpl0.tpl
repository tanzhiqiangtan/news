<div class="news_item" data-uniquekey="{{uniquekey}}" data-url="{{url}}" data-pageNum="{{pageNum}}" data-index="{{index}}">
    <div class="title">
        <h1>{{title}}</h1>
    </div>
    <div class="info">
        <span class="author">{{author}}</span>
        <span>{{date}}</span>
    </div>
</div>
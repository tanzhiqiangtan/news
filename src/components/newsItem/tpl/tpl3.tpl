<div class="news_item" data-uniquekey="{{uniquekey}}" data-url="{{url}}" data-pageNum="{{pageNum}}" data-index="{{index}}">
    <div class="title">
        <h1>{{title}}</h1>
    </div>
    <div class="pic tpl3">
        <div class="img">
            <img src="{{pic1}}" class="pic_show" alt="{{title}}">
        </div>
        <div class="img">
            <img src="{{pic2}}" class="pic_show" alt="{{title}}">
        </div>
        <div class="img last">
            <img src="{{pic3}}" class="pic_show" alt="{{title}}">
        </div>
    </div>
    <div class="info">
        <span class="author">{{author}}</span>
        <span>{{date}}</span>
    </div>
</div>